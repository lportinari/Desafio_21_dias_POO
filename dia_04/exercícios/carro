// Exerc√≠cio em Java

public class Carro {
    public String modelo;
    public String marca;
    private int motor;

    public Carro(String nome, String marca, int motor) {
        this.modelo = modelo;
        this.marca = marca;
        this.motor = motor;
    }

    public String getModelo() {
        return modelo;
    }

    public void setModelo(String modelo) {
        this.modelo = modelo
    }

    public String getMarca() {
        return marca;
    }

    public void setMoarca(String marca) {
        this.marca = marca
    }

    public int getMotor() {
        return motor;
    }

    public void setMoarca(int motor) {
        this.motor = motor
    }
}